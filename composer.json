{
    "name": "api-skeletons/utf8convert",
    "description": "A tool to facilitate a MySQL database conversion to correct invalid utf characters in a database",
    "type": "application",
    "license": "proprietary",
    "keywords": [
        "mysql",
        "database",
        "character set",
        "utf8",
        "utf88mb4"
    ],
    "homepage": "https://github.com/api-skeletons/utf8Convert",
    "support": {
        "issues": "https://www.bluebox.net/insight/blog-article/getting-out-of-mysql-character-set-hell"
    },
    "config": {
        "process-timeout": 5000,
        "secure-http": false
    },
    "require": {
        "php": ">=7.2",
        "zendframework/zend-component-installer": "^1.0",
        "zendframework/zend-stdlib": "^3.2",
        "zendframework/zend-session": "^2.9",
        "zendframework/zend-log": "^2.11",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-router": "3.3.0",
        "zendframework/zend-i18n": "^2.10",
        "zendframework/zend-db": "^2.10",
        "doctrine/doctrine-orm-module": "^2.1",
        "zfcampus/zf-development-mode": "~2.0",
        "beberlei/DoctrineExtensions": "dev-master",
        "zendframework/zend-mvc-console": "^1.2"
    },
    "require-dev": {
        "zendframework/zftool": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src",
            "Db\\": "module/Db/src/Db",
            "Database\\": "module/Database/src/Database"
        }
    }
}
