<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\Conversion">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" nullable="true"/>
    <field name="description" type="text" nullable="true"/>
    <field name="createdAt" type="datetime" nullable="true"/>
    <field name="completedAt" type="datetime" nullable="true"/>
    <one-to-many field="dataPoint" target-entity="Db\Entity\DataPoint" mapped-by="conversion"/>
    <many-to-many field="table" target-entity="Db\Entity\Table" mapped-by="conversion"/>
  </entity>
</doctrine-mapping>
