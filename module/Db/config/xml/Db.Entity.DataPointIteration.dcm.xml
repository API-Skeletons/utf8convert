<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\DataPointIteration" repository-class="Db\Repository\DataPointIterationRepository">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="value" type="text" nullable="true"/>
    <field name="iteration" type="integer" nullable="true"/>
    <many-to-one field="dataPoint" target-entity="Db\Entity\DataPoint" inversed-by="dataPointIteration">
      <join-columns>
        <join-column name="data_point_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
