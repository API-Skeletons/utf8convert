<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\PrimaryKey">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="column" type="string" nullable="true"/>
    <many-to-one field="table" target-entity="Db\Entity\Table" inversed-by="primaryKey">
      <join-columns>
        <join-column name="table_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <one-to-many field="dataPointPrimaryKey" target-entity="Db\Entity\DataPointPrimaryKey" mapped-by="primaryKey"/>
  </entity>
</doctrine-mapping>
