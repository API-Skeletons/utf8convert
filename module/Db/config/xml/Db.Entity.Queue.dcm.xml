<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\Queue" repository-class="Queue\Repository\QueueRepository" table="Queue_Queue">
    <id name="id" type="bigint">
      <generator strategy="AUTO"/>
    </id>
    <field name="queue" type="string" length="64" nullable="false"/>
    <field name="data" type="text" nullable="false"/>
    <field name="status" type="smallint" length="1" nullable="false"/>
    <field name="created" type="datetime" nullable="false"/>
    <field name="scheduled" type="datetime" nullable="false"/>
    <field name="executed" type="datetime" nullable="true"/>
    <field name="finished" type="datetime" nullable="true"/>
    <field name="message" type="text" nullable="true"/>
    <field name="trace" type="text" nullable="true"/>
  </entity>
</doctrine-mapping>
