<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\Table">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" nullable="true"/>
    <one-to-many field="primaryKey" target-entity="Db\Entity\PrimaryKey" mapped-by="table"/>
    <one-to-many field="column" target-entity="Db\Entity\Column" mapped-by="table"/>
    <many-to-many field="conversion" target-entity="Db\Entity\Conversion" inversed-by="table">
      <join-table name="ConversionToTable">
        <join-columns>
          <join-column name="table_id" referenced-column-name="id" nullable="false"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="conversion_id" referenced-column-name="id" nullable="false"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
